    <Ns> [ ", " <throwaway.clause> "," ] <Vst> ( <Ns> | <Np> ) [ "; " <subject.clause> ]
  | <Ns> [ " (" <intro.clause> ")" ] <Vsi> [ <how.clause> ]
  | <Np> [ ", " <throwaway.clause> "," ] <Vpt> [ ", " <how.clause> "," ] ( <Ns> | <Np> )
  | <Np> [ " (" <intro.clause> ")" ] <Vpi> [ ", " <how.clause> ] [ "; " <subject.clause> ]
  ;
